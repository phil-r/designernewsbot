{
  "name": "designernewsbot",
  "version": "1.0.0",
  "author": "Phil Rukin <philipp@rukin.me> (https://rukin.me)",
  "description": "Telegram bot that posts new hot stories from Designer News, shots from Dribbble and projects from Behance to telegram channel",
  "main": "src/index.ts",
  "license": "MIT",
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.6.5",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.8.1",
    "prisma": "^4.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "lru-cache": "^7.13.2",
    "morgan": "^1.10.0"
  },
  "scripts": {
    "dev": "tsnd --respawn src/index.ts",
    "start": "ts-node src/index.ts",
    "start:js": "node dist/index.js",
    "build": "tsc",
    "cron": "ts-node src/cron.ts",
    "migrate": "prisma migrate",
    "migrate:dev": "prisma migrate dev",
    "generate": "prisma generate",
    "studio": "prisma studio",
    "prisma": "prisma",
    "sync": "./hidden/deploy.sh",
    "prepare": "husky install",
    "lint-staged": "lint-staged"
  }
}
